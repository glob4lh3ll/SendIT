{
  "name": "send-it",
  "version": "1.0.0",
  "description": "SendIT is a courier service that helps users deliver parcels to different destinations.",
  "main": "index.js",
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "build": "rm -rf dist/ && babel server/ -d dist/ --ignore server/test",
    "dev": "nodemon --exec babel-node server",
    "dev-test": "nodemon --exec npm test",
    "start": "npm run build && node dist",
    "test": "nyc mocha ./server/**/*test.js --timeout 10000 --require babel-register --exit",
    "coveralls": "npm run test -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/un-tethered/SendIT.git"
  },
  "keywords": [
    "parcel",
    "delivery"
  ],
  "author": "Omoefe Dukuye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/un-tethered/SendIT/issues"
  },
  "homepage": "https://github.com/un-tethered/SendIT#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "make-runnable": "^1.3.6",
    "pg": "^7.6.1",
    "request": "^2.88.0",
    "validator": "^10.9.0"
  }
}
